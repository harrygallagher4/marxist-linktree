---
export interface Props {
  title: string;
};
const { title } = Astro.props;
---

<!doctype html>
<html>
  <head>
    <meta charset='utf-8' />
    <meta name='description' content='Marxist Linktree (dev)' />
    <link rel='icon' type='image/svg+xml' href='/favicon.svg' />
    <meta name='generator' content={Astro.generator} />
    <title>{title}</title>
  </head>
  <body>
    <slot />
  </body>
</html>

<style is:global>
</style>

